  "author": "Ronnie Schaniel <ronnieschaniel@gmail.com>",
  "description": "A project for the supersede web feedback library",
  "license": "tbd",
  "keywords": [
    "jquery",
    "feedback",
    "plugin"
  ],
  "devDependencies": {
    "async": "^1.4.2",
    "browserify": "^13.0.0",
    "chalk": "^1.1.3",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.5.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-filter": "^2.0.2",
    "gulp-ignore": "2.0.1",
    "gulp-inject": "^1.3.1",
    "gulp-inline-ng2-template": "^0.0.7",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "~1.0.1",
    "gulp-sass": "^2.0.4",
    "gulp-shell": "~0.4.3",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-template": "^3.0.0",
    "gulp-tslint": "^3.3.0",
    "gulp-tslint-stylish": "^1.0.4",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "~2.8.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.2.4",
    "handlebars": "^4.0.5",
    "http-server": "^0.7.4",
    "jquery": "^2.1.1",
    "jshint": "^2.5.5",
    "node-http-server": "^1.6.2",
    "normalize.css": "^3.0.1",
    "run-sequence": "^1.1.5",
    "typescript": "^1.8.9",
    "typings": "^0.7.12",
    "uglify-js": "^2.4.15"
  },
  "dependencies": {
    "jquery-ui": "^1.10.5"
  },
  "scripts": {
    "prestart": "npm install && ./node_modules/uglify-js/bin/uglifyjs app/js/jquery.feedback.js -o dist/jquery.feedback.min.js && ./node_modules/uglify-js/bin/uglifyjs app/js/jquery.star-rating-svg.min.js -o dist/jquery.star-rating-svg.min.js",
    "start": "./node_modules/.bin/http-server -a 0.0.0.0 -p 3000 -i",
    "pretest": "npm install && ./node_modules/.bin/jshint js/",
    "test": "./node_modules/.bin/mocha test/spec/tests.js",
    "clean": "rm -rf node_modules && npm install"
  },
  "jshintConfig": {
    "browser": true,
    "node": true,
    "unused": true,
    "undef": true,
    "globals": {
      "$": false,
      "jQuery": false
    }
  },
  "preferGlobal": true
}
